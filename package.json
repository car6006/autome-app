{
  "name": "autome-application",
  "version": "1.0.0",
  "description": "AUTO-ME: Voice & Document Capture with AI Analysis",
  "main": "index.js",
  "scripts": {
    "install-deps": "cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "dev-frontend": "cd frontend && npm run dev",
    "dev-backend": "cd backend && uvicorn server:app --reload --host 0.0.0.0 --port 8001",
    "build-frontend": "cd frontend && npm run build",
    "start-frontend": "cd frontend && npm start",
    "deploy": "npm run build-frontend"
  },
  "keywords": [
    "voice-transcription",
    "ocr",
    "ai-analysis",
    "document-processing",
    "react",
    "fastapi",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/autome-app.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  }
}